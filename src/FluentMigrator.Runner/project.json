{
  "version": "1.6.901-*",
  "authors": [ "Volkan Ceylan" ],
  "description": "This is a temporary .NET Core port of FluentMigrator for Serenity applications. It will only be used until an official version is out.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "https://github.com/volkanceylan/fluentmigrator"
    }
  },
  "dependencies": {
    "FluentMigrator": {
      "target": "project"
    }
  },
  "frameworks": {
    "netstandard1.6": {
      "buildOptions": {
        "define": [
          "COREFX",
          "NETSTANDARD1_6"
        ]
      },
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Data.Common": "4.3.0",
        "System.Data.SqlClient": "4.3.0",
        "System.Reflection.TypeExtensions": "4.3.0",
        "System.Runtime.Loader": "4.0.0",
        "System.Runtime.Serialization.Primitives": "4.3.0"
      }
    },
    "net35": {
      "frameworkAssemblies": {
        "System": "2.0.0.0",
        "System.Core": "3.5.0.0",
        "System.Configuration": "2.0.0.0",
        "System.Xml.Linq": "3.5.0.0",
        "System.Data.DataSetExtensions": "3.5.0.0",
        "System.Data": "2.0.0.0",
        "System.Xml": "2.0.0.0"
      }
    }
  },
  "buildOptions": {
    "compile": {
      "excludeFiles": []
    }
  }
}